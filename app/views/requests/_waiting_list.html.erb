<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope='col'>Order</th>
      <th scope='col'>Name</th>
      <th scope='col'>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @requests.each do |request| %>
      <tr>
        <th scope='row'><%= request.user.wait_order %></th>
          <td><%= request.user.name %></td>
          <td><%= request.user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
